<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Parcial1_V2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Parcial1_V2.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="Parcial_V2">

    <TabBar>

        <ShellContent
            Title="Inicio"
            ContentTemplate="{DataTemplate local:InicioPage}" />

        <ShellContent
            Title="Biografía"
            ContentTemplate="{DataTemplate local:BiografiaPage}" />
 
        <ShellContent
            Title="Discografía"
            ContentTemplate="{DataTemplate local:DiscografiaPage}" />

        <ShellContent
            Title="Tours Pasados"
            ContentTemplate="{DataTemplate local:PastToursPage}" />

        <ShellContent
            Title="Próximos Tours"
            ContentTemplate="{DataTemplate local:UpcomingToursPage}" />

    </TabBar>

</Shell>